{
  "name": "javascript-driver",
  "version": "1.1.0",
  "description": "Babelfish driver for JavaScript",
  "main": "lib/index.js",
  "bin": {
    "javascript-driver": "./lib/index.js"
  },
  "repository": "https://github.com/bblfsh/javascript-driver",
  "author": "source{d}",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && rollup -c",
